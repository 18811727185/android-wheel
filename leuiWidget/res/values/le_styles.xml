<resources>

    <style name="Widget.Leui" parent="@android:Widget.DeviceDefault.Light">
    </style>

    <!-- To Make SourceHanSans font as default theme text font -->
    <style name="Widget.Leui.TextView" parent="@android:Widget.DeviceDefault.Light.TextView">
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="Widget.Leui.EditText" parent="@android:Widget.DeviceDefault.Light.EditText">
        <item name="android:includeFontPadding">false</item>
    </style>
    
    <style name="Widget.Leui.EditText.Multiline">
        <item name="android:padding">10dp</item>
        <item name="android:lineSpacingMultiplier">1.6</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColorHint">#daddde</item>
        <item name="android:textColor">#38454f</item>
        <item name="android:background">@drawable/le_shape_edittext_multiline</item>
    </style>

    <style name="Widget.Leui.LeListView">
        <item name="leSwipeMode">both</item>
        <item name="leSwipeActionLeft">reveal</item>
        <item name="leSwipeActionRight">reveal</item>
        <item name="leSwipeOffsetLeft">0dip</item>
        <item name="leSwipeOffsetRight">0dip</item>
        <item name="leSwipeOpenOnLongPress">true</item>
        <item name="leSwipeAnimationTime">0</item>
        <item name="leSwipeCloseAllItemsWhenMoveList">true</item>
        <item name="leSortDragScrollStart">0.33</item>
        <item name="leSortFloatAlpha">0.6</item>
        <item name="leSortMaxDragScrollSpeed">0.5</item>
        <item name="leSortSlideShuffleSpeed">0.3</item>
        <item name="leSortUseDefaultController">true</item>
        <item name="leSortEnabled">true</item>
    </style>

    <style name="Widget.Leui.StaggeredGridView">
        <item name="android:scrollbarStyle">outsideOverlay</item>
        <item name="android:scrollbars">vertical</item>
    </style>

    <style name="Widget.Leui.ListView.DropDown" parent="@android:Widget.DeviceDefault.Light.ListView.DropDown" >
        <item name="android:divider">@drawable/le_list_dropdown_divider</item>
        <item name="android:scrollbars">none</item>
    </style>

    <style name="Widget.Leui.TextView.SpinnerItem" parent="@android:Widget.DeviceDefault.Light.TextView.SpinnerItem" >
          <item name="android:textAppearance">@style/TextAppearance.Leui.Widget.TextView.SpinnerItem</item>
        <item name="android:paddingStart">0dp</item>
        <item name="android:paddingEnd">0dp</item>
        <item name="android:includeFontPadding">false</item>
    </style>


    <style name="Widget.Leui.DropDownItem" parent="@android:Widget.DeviceDefault.Light.DropDownItem" >
    </style>

    <style name="Widget.Leui.DropDownItem.Spinner" parent="@android:Widget.DeviceDefault.Light.DropDownItem.Spinner" >
        <item name="android:textAppearance">@style/TextAppearance.Leui.Widget.TextView.SpinnerDropdownItem</item>
        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingEnd">16dp</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <!-- popup style start by dongshangyong<dongshangyong@letv.com> -->
    <style name="Widget.Leui.PopupWindow" parent="@android:Widget.DeviceDefault.Light.PopupWindow" >
        <item name="android:popupBackground">@drawable/le_popup_bg</item>
    </style>
